<main>
    <div class="image-container">
        <div class="text">PROJECT NO.2</div>
    </div>

    <div class="container">
        <div class="row">
            {{#each cards}}
            <div class="col s12 m4">
                <div class="card">
                    <div class="card-image">
                        <img src="/images/{{imgFile}}.jpg" width="100%">
                        <span class="card-title">{{title}}</span>
                    </div>
                    <div class="card-content">
                        <p>{{content}}</p>
                    </div>
                </div>
            </div>
             {{/each}}
        </div>
    </div>
</main>

<script src="/js/index.js"></script>